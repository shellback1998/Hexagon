<?xml version="1.0" encoding="UTF-8"?>
<REPORT_QUERY Name="Piping Stress Analysis Dry Weight in kg" Description="Dry Weight per part (Kg), for export to stress analysis" RequiresFilter="No">
	<DESIGN_TIME Progid="SP3DReportsQueryBuilder.SQLQuery" Action="" Arg=""/>
	<RUN_TIME Progid="SP3DRuntimeQuery.CQueryInterpreter" Action="" Arg=""/>
	<SQL Timeout="0">
		<![CDATA[ 
         SELECT jwcg.DryWeight AS Weight
         FROM JWeightCG jwcg 
         WHERE jwcg.oid = ? 
        ]]>
	</SQL>
	<QUERY_PARAMETERS Name="Piping Stress Analysis Dry Weight in kg" Site="User" Path="Piping Stress Analysis Dry Weight in kg.rqp"/>
</REPORT_QUERY>
